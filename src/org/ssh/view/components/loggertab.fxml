<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.collections.*?> 
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.control.ChoiceBox?>

<TableView id="loggertab" fx:id="root" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1" pickOnBounds="false">

	<fx:define>
		<Double fx:value="1.7976931348623157E308" fx:id="maxValue" />
	</fx:define>

	<columnResizePolicy>
		<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
	</columnResizePolicy>
	<columns>
		<TableColumn fx:id="id" text="ID" maxWidth="45" minWidth="45">
			<cellValueFactory>
				<PropertyValueFactory property="id" />
			</cellValueFactory>
		</TableColumn>
		<TableColumn text="Level" maxWidth="105" minWidth="105">
			<graphic>
				<ChoiceBox fx:id="levelDropdown" value="INFO">
					<items>
					<FXCollections fx:factory="observableArrayList">
						<String fx:value="SEVERE" />
						<String fx:value="WARNING" />
						<String fx:value="INFO" />
						<String fx:value="CONFIG" />
						<String fx:value="FINE" />
						<String fx:value="FINER" />
						<String fx:value="FINEST" />
					</FXCollections>
					</items>
				</ChoiceBox>
			</graphic>
			<cellValueFactory>
				<PropertyValueFactory property="level" />
			</cellValueFactory>
		</TableColumn>
		<TableColumn text="Location" maxWidth="130" minWidth="130">
			<cellValueFactory>
				<PropertyValueFactory property="location" />
			</cellValueFactory>
		</TableColumn>
		<TableColumn text="Time" maxWidth="68" minWidth="68">
			<cellValueFactory>
				<PropertyValueFactory property="time" />
			</cellValueFactory>
		</TableColumn>
		<TableColumn text="Description">
			<cellValueFactory>
				<PropertyValueFactory property="description" />
			</cellValueFactory>
		</TableColumn>
	</columns>
	<items>
		<FXCollections fx:factory="observableArrayList">
		</FXCollections>
	</items>
	<sortOrder>
		<fx:reference source="id" />
	</sortOrder>

</TableView>