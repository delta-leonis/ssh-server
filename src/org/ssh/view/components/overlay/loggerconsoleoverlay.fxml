<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<GridPane id="overlayLogconsole" pickOnBounds="false" xmlns="http://javafx.com/javafx/8"
          xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints percentWidth="25.0"/>
        <ColumnConstraints minWidth="30.0" maxWidth="30.0"/>
        <ColumnConstraints fillWidth="true"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints percentHeight="6.0"/>
        <RowConstraints percentHeight="14.0"/>
        <RowConstraints percentHeight="35"/>
        <RowConstraints percentHeight="45"/>
    </rowConstraints>
    <children>
        <Pane fx:id="loggingconsolecollapsedWrapper" pickOnBounds="false" GridPane.hgrow="ALWAYS"
              GridPane.vgrow="ALWAYS"
              GridPane.columnIndex="2" GridPane.rowIndex="1"/>
        <Pane fx:id="loggingconsoleextendedWrapper" pickOnBounds="false" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS"
              GridPane.columnIndex="2" GridPane.rowIndex="1" GridPane.rowSpan="2"/>
        <Button id="enrollLogconsole" maxHeight="Infinity" GridPane.columnIndex="1" GridPane.rowIndex="1"
                GridPane.vgrow="ALWAYS" onAction="#enrollLoggingconsole">
            <graphic>
                <ImageView fx:id="enrollLogconsoleImage">
                    <Image url="@../../icon/enroll16.png"/>
                </ImageView>
            </graphic>
        </Button>

    </children>
</GridPane>