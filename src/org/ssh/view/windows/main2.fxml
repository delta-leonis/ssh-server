<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<StackPane fx:id="rootNode" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">

	<fx:define>
		<Double fx:value="1.7976931348623157E308" fx:id="maxValue" />
	</fx:define>

	<children>
	<GridPane id="basePane" fx:id="basePane">
		<columnConstraints>
			<ColumnConstraints percentWidth="100.0" />
		</columnConstraints>
		<rowConstraints>
			<RowConstraints percentHeight="20.0" />
			<RowConstraints percentHeight="73.0" />
			<RowConstraints percentHeight="7.0" />
		</rowConstraints>
		<children>

		<GridPane id="baseBottom" fx:id="baseBottom" GridPane.columnIndex="0" GridPane.rowIndex="2">
			<columnConstraints>
					<ColumnConstraints fillWidth="true" />
					<ColumnConstraints fillWidth="true" />
					<ColumnConstraints fillWidth="true" />
				</columnConstraints>
				<rowConstraints>
					<RowConstraints percentHeight="100.0" />
				</rowConstraints>
				
				<children>
				<Button id="enrollButton" maxHeight="${maxValue}" GridPane.columnIndex="0" GridPane.rowIndex="0">
					<graphic>
						<ImageView fx:id="enrollToolboxImage">
							<image>
								<Image url="@../icon/enrollup32.png" />
							</image>
						</ImageView>
					</graphic>
				</Button>
				<Pane fx:id="timesliderWrapper" maxHeight="${maxValue}" maxWidth="${maxValue}" GridPane.hgrow="ALWAYS" GridPane.columnIndex="1" GridPane.rowIndex="0"></Pane>
				<Button id="secondscreenButton" maxHeight="${maxValue}" GridPane.columnIndex="2" GridPane.rowIndex="0">
					<graphic>
						<ImageView>
							<image>
								<Image url="@../icon/secondscreen32.png" />
							</image>
						</ImageView>
					</graphic>
				</Button>
				</children>
			</GridPane>
		</children>
	</GridPane>

	<GridPane id="overlay" fx:id="overlay" pickOnBounds="false">
		<columnConstraints>
			<ColumnConstraints percentWidth="20.0" />
			<ColumnConstraints percentWidth="80.0" />
		</columnConstraints>
		<rowConstraints>
			<RowConstraints percentHeight="6.0" />
			<RowConstraints percentHeight="30.0" />
			<RowConstraints percentHeight="64.0" />
		</rowConstraints>
		<children>
		<Pane fx:id="profilemenuWrapper" pickOnBounds="false" GridPane.columnIndex="0" GridPane.rowIndex="1"></Pane>
		</children>
	</GridPane>
	
	<GridPane id="overlayLogconsole" pickOnBounds="false">
		<columnConstraints>
			<ColumnConstraints percentWidth="25.0" />
			<ColumnConstraints minWidth="30.0" maxWidth="30.0" prefWidth="30.0" />
			<ColumnConstraints fillWidth="true" />
		</columnConstraints>
		<rowConstraints>
			<RowConstraints percentHeight="6.0" />
			<RowConstraints percentHeight="64.0" />
			<RowConstraints percentHeight="30.0" />
		</rowConstraints>
		<children>
		<Pane fx:id="loggingconsoleWrapper" pickOnBounds="false" maxHeight="${maxValue}" maxWidth="${maxValue}" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" GridPane.columnIndex="2" GridPane.rowIndex="1" />
		</children>
	</GridPane>
	</children>
</StackPane>
